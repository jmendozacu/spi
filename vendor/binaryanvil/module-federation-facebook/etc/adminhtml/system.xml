<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <section id="binaryanvil_federation">
            <group id="facebook" translate="label" type="text" sortOrder="20" showInDefault="1" showInWebsite="0" showInStore="0">
                <label><![CDATA[
                    <img style="vertical-align: middle; display: inline-block; height: 32px; margin:0 0 3px 0;" alt="Facebook Login" src="data:image/png;base64,"/>
                    Facebook Login
                ]]></label>
                <field id="is_enabled" translate="label" type="select" sortOrder="20" showInDefault="1" showInWebsite="0" showInStore="0">
                    <label>Enable Login with Facebook</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="api_key" translate="label" type="text" sortOrder="30" showInDefault="1" showInWebsite="0" showInStore="0">
                    <label>Facebook App ID</label>
                </field>
                <field id="api_secret" translate="label" type="text" sortOrder="40" showInDefault="1" showInWebsite="0" showInStore="0">
                    <label>Facebook App Secret</label>
                </field>
                <field id="api_access_token" translate="label" type="text" sortOrder="50" showInDefault="1" showInWebsite="0" showInStore="0">
                    <label>Facebook Access Token</label>
                </field>
            </group>
            <group id="facebook_like" translate="label" type="text" sortOrder="21" showInDefault="1" showInWebsite="0" showInStore="0">
                <label><![CDATA[
                    <img style="vertical-align: middle; display: inline-block; height: 32px; margin:0 0 3px 0;" alt="Facebook Like" src="data:image/false;base64,"/>
                    Facebook Like
                ]]></label>
                <field id="is_enabled" translate="label" type="select" sortOrder="20" showInDefault="1" showInWebsite="0" showInStore="0">
                    <label>Enable Facebook Like</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                    <frontend_model>BinaryAnvil\FederationFacebook\Block\Adminhtml\Hint</frontend_model>
                </field>
            </group>
            <group id="facebook_social_feed" translate="label" type="text" sortOrder="22" showInDefault="1" showInWebsite="0" showInStore="0">
                <label><![CDATA[
                    <img style="vertical-align: middle; display: inline-block; height: 25px; margin:0 1px 3px 0;" alt="Facebook Feed" src="data:image/png;base64,"/>
                    Facebook Social Feed
                ]]></label>
                <field id="is_enabled" translate="label" type="select" sortOrder="10" showInDefault="1" showInWebsite="0" showInStore="0">
                    <label>Enable Facebook Social Feed</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="feed_mode" translate="label" type="select" sortOrder="20" showInDefault="1" showInWebsite="0" showInStore="0">
                    <label>Feed Mode</label>
                    <source_model>BinaryAnvil\FederationFacebook\Model\Source\Config\FeedMode</source_model>
                    <depends>
                        <field id="binaryanvil_federation/facebook_social_feed/is_enabled">1</field>
                    </depends>
                </field>
                <field id="page_id" translate="label" type="text" sortOrder="30" showInDefault="1" showInWebsite="0" showInStore="0">
                    <label>Page ID</label>
                    <depends>
                        <field id="binaryanvil_federation/facebook_social_feed/is_enabled">1</field>
                        <field id="binaryanvil_federation/facebook_social_feed/feed_mode">!</field>
                    </depends>
                </field>
                <field id="user" translate="label" type="text" sortOrder="30" showInDefault="1" showInWebsite="0" showInStore="0">
                    <label>User</label>
                    <depends>
                        <field id="binaryanvil_federation/facebook_social_feed/is_enabled">1</field>
                        <field id="binaryanvil_federation/facebook_social_feed/feed_mode">@</field>
                    </depends>
                </field>
                <field id="posts_limit" translate="label" type="text" sortOrder="40" showInDefault="1" showInWebsite="0" showInStore="0">
                    <label>Posts Limit</label>
                    <depends>
                        <field id="binaryanvil_federation/facebook_social_feed/is_enabled">1</field>
                    </depends>
                </field>
                <field id="is_cache_enabled" translate="label" type="select" sortOrder="50" showInDefault="1" showInWebsite="0" showInStore="0">
                    <label>Enable Cache</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                    <depends>
                        <field id="binaryanvil_federation/facebook_social_feed/is_enabled">1</field>
                    </depends>
                </field>
                <field id="cache_lifetime" translate="label" type="select" sortOrder="51" showInDefault="1" showInWebsite="0" showInStore="0">
                    <label>Cache Lifetime</label>
                    <source_model>BinaryAnvil\FederationFacebook\Model\Source\Config\Cache\Lifetime</source_model>
                    <depends>
                        <field id="binaryanvil_federation/facebook_social_feed/is_enabled">1</field>
                        <field id="binaryanvil_federation/facebook_social_feed/is_cache_enabled">1</field>
                    </depends>
                </field>
            </group>
        </section>
    </system>
</config>
